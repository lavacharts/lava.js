<!doctype html>
<html>
<head>
  {{> partials/head }}
</head>

<body>
  <nav class="light-blue lighten-1" role="navigation">
    {{> partials/navbar }}
  </nav>

  <div class="section no-pad-bot" id="index-banner">
    {{> partials/banner }}
  </div>

  <div class="container">
    <div class="section">
      <div class="row">
        <!-- #region Code Box -->
        <div class="col s12 m6">
          <div class="icon-block">
            <h2 class="center light-blue-text">
                Easy Syntax
            </h2>
            <h5 class="center">clean & consise</h5>
            <pre class="language-javascript">
              <code>
{{ exampleCode }}</code>
            </pre>
          </div>
        </div>
        <!-- #endregion -->

        <!-- #region Chart Box -->
        <div class="col s12 m6">
          <div class="icon-block">
            <h2 class="center light-blue-text">
                Easy Charts
            </h2>
            <h5 class="center">thanks google!</h5>
            <pre class="language-html">
              <code>
&#x3C;div id=&#x22;chart_div&#x22;&#x3E;&#x3C;/div&#x3E;</code>
            </pre>
            <div id="chart_div"></div>
          </div>
        </div>
        <!-- #endregion -->

      </div><!-- </div class="row"> -->
    </div>

    <div class="section">
      <div class="row">
      </div>
    </div>
  </div>

  <footer class="page-footer orange">
      {{> partials/footer }}
  </footer>
</body>
</html>

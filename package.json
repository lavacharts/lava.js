{
  "name": "@lavacharts/lava.js",
  "title": "LavaJs",
  "version": "4.0.0-beta1",
  "description": "Wrapper library for the Google Chart API",
  "private": false,
  "main": "dist/lava.js",
  "unpkg": "dist/lava.js",
  "browser": "dist/lava.js",
  "jsdelivr": "dist/lava.js",
  "files": [
    "dist"
  ],
  "browserslist": [
    "last 2 versions",
    "> 1%"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build-site": "yarn run rm:public && concurrently npm:build:*",
    "build:api": "typedoc --out ./public/api ./src",
    "build:docs": "webpack --config ./configs/dev/webpack.dev.js",
    "bundle": " bili --format cjs --format esm",
    "dev": "webpack-dev-server --config ./configs/dev/webpack.dev.js",
    "lint": "eslint \"./src/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "publish:gh-pages": "gh-pages -d public/",
    "prod": "yarn run production",
    "production": "webpack --config ./configs/webpack.prod.js",
    "start": "yarn run build && yarn run publish:github",
    "test": "karma start --singleRun ./karma.config.js",
    "test:chrome": "yarn test --browsers Chrome",
    "test:firefox": "yarn test --browsers Firefox",
    "typecheck": "tsc --noEmit",
    "rm:public": "yarn run rimraf ./public/*"
  },
  "lint-staged": {
    "./src/**/*.ts": [
      "yarn lint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": "git+https://github.com/lavacharts/lava.js.git",
  "keywords": [
    "google",
    "charts",
    "graphs",
    "lavacharts",
    "javascript",
    "typescript"
  ],
  "author": "Kevin Hill",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lavacharts/lava.js/issues"
  },
  "homepage": "https://lavacharts.github.io/lava.js/",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "^7.6.3",
    "@types/debug": "^4.1.5",
    "@types/node": "^12.7.5",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "bili": "^4.10.0",
    "chai": "^4.2.0",
    "chai-shallow-deep-equal": "^1.4.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "core-js": "3",
    "css-loader": "^3.2.0",
    "cypress": "^4.5.0",
    "debug": "^4.1.1",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-simple-import-sort": "^4.0.0",
    "file-loader": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gh-pages": "^2.1.1",
    "handlebars": "^4.4.3",
    "handlebars-loader": "^1.7.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "jquery": "^3.4.1",
    "karma": "^5.0.7",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-viewport": "^0.4.2",
    "karma-webpack": "^4.0.2",
    "lint-staged": "^9.4.1",
    "lodash": "^4.17.15",
    "materialize-css": "^1.0.0-rc.2",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^6.2.1",
    "node-sass": "^4.12.0",
    "prettier": "^2.0.5",
    "prismjs": "^1.17.1",
    "regenerator-runtime": "^0.13.3",
    "resolve-url-loader": "^3.1.0",
    "rimraf": "^3.0.0",
    "rollup-plugin-dts": "^1.4.6",
    "rollup-plugin-typescript2": "^0.27.1",
    "sass-loader": "^8.0.0",
    "string-replace-loader": "^2.2.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^1.0.0",
    "thread-loader": "^2.1.3",
    "tiny-emitter": "^2.1.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-error-notification": "^0.1.8",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}

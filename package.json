{
  "name": "@lavacharts/lava.js",
  "version": "4.0.0-beta3",
  "description": "Wrapper library for the Google Chart API",
  "homepage": "http://lavacharts.com",
  "author": "Kevin Hill",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/lavacharts/lava.js.git"
  },
  "bugs": {
    "url": "https://github.com/lavacharts/lava.js/issues"
  },
  "main": "./dist/lava.js",
  "scripts": {
    "dev": "yarn run webpack-dev-server --config ./configs/webpack.dev.js",
    "ts-check": "yarn run tsc --noEmit",
    "test": "yarn run karma start --singleRun",
    "lint": "yarn run eslint --ext=.ts src/",
    "prod": "yarn run build:prod",
    "deploy": "yarn build && yarn run gh-pages -d public/",
    "build": "yarn run build:pages",
    "build:prod": "yarn run parallel-webpack --config ./configs/webpack.prod.js",
    "build:pages": "yarn run webpack --config ./configs/webpack.dev.js",
    "build:lava": "yarn run webpack --config ./configs/webpack.lava.js",
    "build:lavacharts": "yarn run webpack --config ./configs/webpack.lavacharts.js"
  },
  "lint-staged": {
    "./src/**/*.ts": [
      "yarn lint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "redux-zero": "^5.1.0",
    "tiny-emitter": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/runtime": "^7.6.3",
    "@types/google.visualization": "^0.0.48",
    "@types/node": "^12.7.5",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.4.0",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "chai-shallow-deep-equal": "^1.4.6",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "3",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-simple-import-sort": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gh-pages": "^2.1.1",
    "handlebars": "^4.4.3",
    "handlebars-loader": "^1.7.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.8",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-nightmare": "^0.4.10",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon": "^1.0.5",
    "karma-sinon-chai": "^1.3.1",
    "karma-viewport": "^0.4.2",
    "lint-staged": "^9.4.1",
    "materialize-css": "^1.0.0-rc.2",
    "mocha": "^6.2.1",
    "mocha-sinon": "^2.0.0",
    "nightmare": "^3.0.2",
    "parallel-webpack": "^2.4.0",
    "prettier": "^1.18.2",
    "prismjs": "^1.17.1",
    "regenerator-runtime": "^0.13.3",
    "sinon": "^7.5.0",
    "sinon-chai": "^2.13.0",
    "string-replace-loader": "^2.2.0",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-error-notification": "^0.1.8",
    "webpack-merge": "^4.2.2"
  }
}

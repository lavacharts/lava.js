{
  "name": "lava.js",
  "title": "LavaJs",
  "version": "4.0.0-alpha1",
  "description": "Wrapper library for the Google Chart API",
  "private": false,
  "main": "dist/lava.js",
  "unpkg": "dist/lava.js",
  "jsdelivr": "dist/lava.js",
  "files": [
    "src",
    "dist/*.js",
    "types/*.d.ts"
  ],
  "directories": {
    "test": "test"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "scripts": {
    "dev": "yarn run webpack-dev-server --config ./configs/dev/webpack.dev.js",
    "deploy": "yarn run build && yarn run gh-pages -d public/",
    "prod": "yarn run clean:dist && yarn run build:prod",
    "build": "yarn run build:prod:lavajs",
    "build-all": "yarn run clean-all && yarn run concurrently npm:build-*",
    "clean-all": "yarn run concurrently npm:clean:*",
    "build-site": "yarn run concurrently npm:build:site:*",
    "build:site:pages": "yarn run webpack --config ./configs/dev/webpack.dev.js",
    "build:site:api": "yarn run typedoc --out ./public/api ./src",
    "build:prod": "yarn run webpack --config ./configs/webpack.prod.js",
    "clean:public": "yarn run rimraf ./public/*",
    "clean:dist": "yarn run rimraf ./dist/*",
    "lint": "yarn run eslint --ext=.ts src/",
    "test": "yarn run karma start --singleRun"
  },
  "lint-staged": {
    "./src/**/*.ts": [
      "yarn lint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lavacharts/lava.js.git"
  },
  "keywords": [
    "google",
    "charts",
    "graphs",
    "lavacharts",
    "javascript",
    "typescript"
  ],
  "author": "Kevin Hill",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lavacharts/lava.js/issues"
  },
  "homepage": "http://lavajs.lavacharts.com",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/runtime": "^7.6.3",
    "@types/debug": "^4.1.5",
    "@types/node": "^12.7.5",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.4.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "chai-shallow-deep-equal": "^1.4.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "core-js": "3",
    "css-loader": "^3.2.0",
    "debug": "^4.1.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-simple-import-sort": "^4.0.0",
    "file-loader": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gh-pages": "^2.1.1",
    "handlebars": "^4.4.3",
    "handlebars-loader": "^1.7.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.8",
    "jquery": "^3.4.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-nightmare": "^0.4.10",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon": "^1.0.5",
    "karma-sinon-chai": "^1.3.1",
    "karma-viewport": "^0.4.2",
    "lint-staged": "^9.4.1",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "materialize-css": "^1.0.0-rc.2",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^6.2.1",
    "mocha-sinon": "^2.0.0",
    "nightmare": "^3.0.2",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "prismjs": "^1.17.1",
    "regenerator-runtime": "^0.13.3",
    "resolve-url-loader": "^3.1.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^2.13.0",
    "string-replace-loader": "^2.2.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^1.0.0",
    "thread-loader": "^2.1.3",
    "tiny-emitter": "^2.1.0",
    "typedoc": "^0.15.0",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-error-notification": "^0.1.8",
    "webpack-merge": "^4.2.2"
  }
}

    <h6 class="center">One Control, One Chart</h6>
  </div>
</div>

<div class="row">
  <div class="col s12 m6">
    <div class="icon-block z-depth-1">
      <div class="icon-block-header">
        <h4 class="center amber-text">Script</h4>
      </div>
      <pre class="language-javascript">
        <code id="one-to-one_code">
{{ exampleCode }}</code>
      </pre>

<div class="row">
  <div class="icon-block">
    <h3 class="center light-blue-text">One to Many</h3>
    <h6 class="center">One Control, Multiple Charts</h6>
  </div>
</div>

<div class="row">
  <div class="col s12 m6">
    <div class="icon-block z-depth-1">
      <div class="icon-block-header">
        <h4 class="center amber-text">Script</h4>
      </div>
      <pre class="language-javascript">
        <code id="one-to-many">{{ exampleCode }}</code>
      </pre>
    </div>
  </div>


  <div class="col s12 m6">
    <div class="icon-block z-depth-1">
      <div class="icon-block-header">
        <h4 class="center amber-text">Markup</h4>
      </div>
      <pre class="language-html"><code>
&lt;!--Div for the dashboard--&gt;
&lt;div id=&quot;dashboard_div&quot;&gt;
    &lt;!--Divs for each control and chart--&gt;
    &lt;div id=&quot;filter_div&quot;&gt;&lt;/div&gt;
    &lt;div id=&quot;pieChart_div&quot;&gt;&lt;/div&gt;
    &lt;div id=&quot;barChart_div&quot;&gt;&lt;/div&gt;
&lt;/div&gt;</code>
      </pre>
    </div>

    <div class="icon-block z-depth-1">
      <div class="icon-block-header">
        <h4 class="center amber-text">Dashboard</h4>
      </div>
      <div id="dashboard_div">
        <div id="filter_div"></div>
        <div id="pieChart_div"></div>
        <div id="barChart_div"></div>
      </div>
    </div>
  </div>
</div>
<script src="one-to-many.js"></script>

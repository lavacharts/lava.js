<!doctype html>
<html>
<head>
  {{> partials/head }}
</head>

<body>
  <nav class="light-blue lighten-1" role="navigation">
    {{> partials/navbar }}
  </nav>
  <div class="section no-pad-bot" id="index-banner">
    <div class="container">
      <h1 class="header center orange-text">Multiple Charts</h1>
      <div class="row center">
        <h5 class="header col s12 light">of course!</h5>
      </div>
    </div>
  </div>
  <div class="container">
    <div class="section">
      <div class="row">
        <div class="col s12 m6">
          <div class="icon-block z-depth-1">
            <div class="icon-block-header">
              <h2 class="center light-blue-text">So Easy</h2>
              <h5 class="center">with easy setters</h5>
            </div>
            <pre class="language-javascript">
              <code>
{{ exampleCode }}</code>
            </pre>
          </div>
        </div>
        <div class="col s12 m6">
          <div class="icon-block z-depth-1">
            <div class="icon-block-header">
              <h2 class="center light-blue-text">Redraw</h2>
              <h5 class="center">with each change</h5>
            </div>
            <pre class="language-html">
            <code>
&lt;div id=&quot;chart_div1&quot;&gt;&lt;/div&gt;
&lt;div id=&quot;chart_div2&quot;&gt;&lt;/div&gt;</code>
            </pre>
            <div id="chart_div1"></div>
            <div id="chart_div2"></div>
          </div>
        </div>
      </div>
    </div>
    <div class="section">
      <div class="row">
        <br />
      </div>
    </div>
  </div>
  <footer class="page-footer">
      {{> partials/footer }}
  </footer>
</body>
</html>
